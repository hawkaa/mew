
@{
    ViewBag.Title = "Index";
}
<!doctype html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    @Styles.Render("~/Content/css")
    <link href='https://fonts.googleapis.com/css?family=Work+Sans:400,200,300|Merriweather:400,300,700' rel='stylesheet' type='text/css'>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top m-navbar">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <div class="navbar-brand">Project Mew</div>
            </div>
            <div class="navbar-collapse collapse m-navbar">
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <!-- Modal -->
    <div class="modal fade" id="test-modal" tabindex="-1" role="dialog" aria-labelledby="modal-label">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                MODAL!
            </div>
        </div>
    </div>

<div id="map"></div>
<div class="new-trip-btn">
    <div id="m-plus" data-toggle="modal" data-target="#test-modal">+</div></div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/Scripts/map/bundle.js")
</body>
</html>

